      <xpath expr="//form/sheet/group" position="inside">
        <field name="vin"></field>
        <button class="btn-primary" name="autocomplete_vehicle_info" string="Autocompletar por VIN" type="object"></button>
      </xpath>
    </field>
  </record>
</odoo>
